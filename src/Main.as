package  {		import adobe.utils.MMExecute;		import flash.display.Sprite;	import flash.events.MouseEvent;	import fl.controls.CheckBox;	import fl.events.ComponentEvent;		public class Main extends Sprite {				public function Main() {						elements.addBackground.addEventListener(MouseEvent.CLICK, _addBackground);			elements.createHero.addEventListener(MouseEvent.CLICK, _createHero);			options.validate.addEventListener(MouseEvent.CLICK, _validate);			options.visible = false;		}				private function _validate(mEvt:MouseEvent):void {						MMExecute("fl.runScript(fl.configURI + 'WindowSWF/CitrusEnginePanels.jsfl', 'addBackground','"+options.path.text+"');");			options.path.text = "";			options.visible = false;			elements.visible = true;		}				private function _addBackground(mEvt:MouseEvent):void {						options.visible = true;			elements.visible = false;		}				private function _createHero(mEvt:MouseEvent):void {						MMExecute("fl.runScript(fl.configURI + 'WindowSWF/CitrusEnginePanels.jsfl', 'createHero');");		}	}}